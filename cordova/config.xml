<?xml version='1.0' encoding='utf-8'?>
<widget id="org.selfobserved.blume" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Blume</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
        <preference name="CodePushDeploymentKey" value="9MMniBGRgssJyu-toCQLAA6gV5js70b36c57-c09c-4a4c-8637-6bd98533ff92" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <preference name="CodePushDeploymentKey" value="Po0UxFCf_IiLDPFB9m8diwtKhQZv70b36c57-c09c-4a4c-8637-6bd98533ff92" />
    </platform>
    <engine name="ios" spec="^4.4.0" />
    <plugin name="cordova-plugin-ble" spec="git+https://github.com/evothings/cordova-ble.git#2.0.1" />
    <plugin name="cordova-plugin-code-push" spec="^1.9.5-beta" />
    <plugin name="cordova-plugin-whitelist" spec="^1.3.2" />
</widget>
